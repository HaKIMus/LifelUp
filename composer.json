{
    "name": "hakim/lifelup",
    "description": "Create a life-quests and get experience for doing it.",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/assert": "^2.9",
        "doctrine/dbal": "^2.9@dev",
        "ramsey/uuid": "^3.0",
        "sensio/framework-extra-bundle": "^5.2@dev",
        "symfony/apache-pack": "dev-master",
        "symfony/asset": "^4.2",
        "symfony/console": "^4.2",
        "symfony/flex": "^1.0@dev",
        "symfony/force-lowest": "=4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/monolog-bundle": "^3.0@dev",
        "symfony/orm-pack": "dev-master",
        "symfony/twig-bundle": "^4.2",
        "symfony/yaml": "^4.2",
        "prooph/event-sourcing": "^5.2",
        "prooph/event-store-bus-bridge": "^3.0",
        "prooph/event-store-http-middleware": "^0.2.0",
        "prooph/event-store-symfony-bundle": "^0.4",
        "prooph/pdo-event-store": "^1.0",
        "prooph/service-bus-symfony-bundle": "^0.6"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0@dev",
        "prooph/php-cs-fixer-config": "^0.2",
        "phpspec/phpspec": "^5.0",
        "symfony/dotenv": "^4.2",
        "symfony/maker-bundle": "^1.0@dev",
        "symfony/profiler-pack": "dev-master",
        "symfony/var-dumper": "^4.2",
        "symfony/web-server-bundle": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "bin-dir": "bin",
        "sort-packages": true
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    },
    "license": "MIT License",
    "authors": [
        {
            "name": "HaKIM",
            "email": "webdevhakim@gmail.com"
        }
    ]
}
